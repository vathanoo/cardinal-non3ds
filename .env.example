# START GENAI
# Visa Payment Passkey (VPP) Configuration
# Copy this file to .env and update with your actual values

# Server Configuration
PORT=3000
NODE_ENV=development
ALLOWED_ORIGINS=http://localhost:3000,https://localhost:3000

# Visa Developer Center (VDC) Configuration
# Obtain these from your VDC project dashboard
VDC_API_KEY=your_vdc_api_key_here
VDC_SHARED_SECRET=your_vdc_shared_secret_here
VDC_USER_ID=your_vdc_user_id_here

# VPP Environment Configuration
# Sandbox URLs for testing
VPP_BASE_URL_SANDBOX=https://sandbox.auth.visa.com
VPP_API_BASE_URL_SANDBOX=https://sandbox.api.visa.com/passkey

# Production URLs (use only when ready for production)
VPP_BASE_URL_PROD=https://auth.visa.com
VPP_API_BASE_URL_PROD=https://api.visa.com/passkey

# Current environment (sandbox or production)
VPP_ENVIRONMENT=sandbox

# Merchant Configuration
MERCHANT_NAME=Your Merchant Name
MERCHANT_ORIGIN=http://localhost:3000
INTEGRATOR_ORIGIN=http://localhost:3000

# VPP Onboarding Configuration
# These values are provided during VPP onboarding
# IMPORTANT: APN must be in lowercase as per VPP specification (Section 2.2)
VPP_APN=your_apn_provided_during_onboarding_in_lowercase
VPP_CLIENT_ID=your_client_id_provided_during_onboarding
VPP_CLIENT_VERSION=your_version_provided_during_onboarding
VPP_PRODUCT_CODE=your_product_code_provided_during_onboarding

# FIDO2 Configuration
FIDO2_TIMEOUT=360000
FIDO2_RP_NAME=VPP Merchant Demo

# Security Configuration
JWT_SECRET=your_jwt_secret_for_session_management
SESSION_TIMEOUT=900

# 2-Way SSL Configuration (Mutual TLS)
VPP_CLIENT_CERT_PATH=certs/cert.pem
VPP_CLIENT_KEY_PATH=certs/privateKey-d0a286b0-a2e4-4720-9943-52c87b5a7d94.pem
VPP_REJECT_UNAUTHORIZED=true

# Basic Authentication for PA API
VPP_BASIC_AUTH_USERNAME=IC2SI1RB5JKAKL5SKJ1T21uoRgNp2xruV18Gc3TpzAYaszxEo
VPP_BASIC_AUTH_PASSWORD=xLe5f0Vypq5H0JD8Ipg7TOr1ltyYHrMn

# Debug Configuration
DEBUG_MODE=true
LOG_LEVEL=info

# END GENAI
